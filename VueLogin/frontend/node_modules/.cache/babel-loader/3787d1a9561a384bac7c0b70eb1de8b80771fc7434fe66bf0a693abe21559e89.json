{"ast":null,"code":"import { reactive } from 'vue';\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const state = reactive({\n      account: {\n        id: null,\n        name: \"\"\n      },\n      form: {\n        loginId: \"\",\n        loginPw: \"\"\n      }\n    });\n    axios.get(\"/api/account\").then(res => {\n      console.log(res);\n      state.account = res.data;\n    });\n    const submit = () => {\n      const args = {\n        loginId: state.form.loginId,\n        loginPw: state.form.loginPw\n      };\n      axios.post(\"/api/account\", args).then(res => {\n        console.log(res);\n        alert(\"로그인에 성공했습니다.\");\n        state.account = res.data;\n      }).catch(() => {\n        alert(\"로그인에 실패했습니다. 계정 정보를 확인하세요.\");\n      });\n    };\n    return {\n      state,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","setup","state","account","id","name","form","loginId","loginPw","get","then","res","console","log","data","submit","args","post","alert","catch"],"sources":["C:\\Users\\kahn\\Desktop\\VUE\\VueLogin\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"app\">\n    <div v-if=\"state.account.id\">안녕하세요? {{state.account.name}}님!!</div>\n    <div v-else>\n      <label for=\"loginId\">\n        <span>아이디</span>\n        <input type=\"text\" id=\"loginId\" v-model=\"state.form.loginId\"/>\n      </label>\n      <label for=\"loginPw\">\n        <span>패스워드</span>\n        <input type=\"text\" id=\"loginPw\" v-model=\"state.form.loginPw\"/>\n      </label>\n      <button @click=\"submit()\">로그인</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive } from 'vue';\nimport axios from \"axios\";\n\nexport default{\n  setup(){\n    const state = reactive({\n      account:{\n        id:null,\n        name:\"\"\n      },\n      \n      form:{\n        loginId:\"\",\n        loginPw:\"\",\n      }\n    })\n\n    axios.get(\"/api/account\").then((res) =>{\n      console.log(res);\n      state.account = res.data;\n\n    })\n\n\n    const submit = () =>{\n      const args = {\n        loginId: state.form.loginId,\n        loginPw: state.form.loginPw,\n      }\n      axios.post(\"/api/account\", args).then((res) =>{\n        console.log(res);\n        alert(\"로그인에 성공했습니다.\")\n        state.account = res.data;\n\n      })\n      .catch(()=>{\n        alert(\"로그인에 실패했습니다. 계정 정보를 확인하세요.\");\n      })\n      \n    }\n   \n\n    return {state, submit}\n  }\n}\n</script>\n\n\n\n"],"mappings":"AAkBA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAc;EACZC,KAAKA,CAAA,EAAE;IACL,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACrBI,OAAO,EAAC;QACNC,EAAE,EAAC,IAAI;QACPC,IAAI,EAAC;MACP,CAAC;MAEDC,IAAI,EAAC;QACHC,OAAO,EAAC,EAAE;QACVC,OAAO,EAAC;MACV;IACF,CAAC;IAEDR,KAAK,CAACS,GAAG,CAAC,cAAc,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAI;MACrCC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAChBT,KAAK,CAACC,OAAM,GAAIQ,GAAG,CAACG,IAAI;IAE1B,CAAC;IAGD,MAAMC,MAAK,GAAIA,CAAA,KAAK;MAClB,MAAMC,IAAG,GAAI;QACXT,OAAO,EAAEL,KAAK,CAACI,IAAI,CAACC,OAAO;QAC3BC,OAAO,EAAEN,KAAK,CAACI,IAAI,CAACE;MACtB;MACAR,KAAK,CAACiB,IAAI,CAAC,cAAc,EAAED,IAAI,CAAC,CAACN,IAAI,CAAEC,GAAG,IAAI;QAC5CC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;QAChBO,KAAK,CAAC,cAAc;QACpBhB,KAAK,CAACC,OAAM,GAAIQ,GAAG,CAACG,IAAI;MAE1B,CAAC,EACAK,KAAK,CAAC,MAAI;QACTD,KAAK,CAAC,4BAA4B,CAAC;MACrC,CAAC;IAEH;IAGA,OAAO;MAAChB,KAAK;MAAEa;IAAM;EACvB;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}