{"ast":null,"code":"import { reactive } from 'vue';\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const state = reactive({\n      data: []\n    });\n    const add = () => {\n      const content = prompt(\"내용을 입력해주세요.\");\n      axios.post(\"/api/memos\", {\n        content\n      }).then(res => {\n        state.data = res.data;\n        console.log(res.data);\n      });\n    };\n    const edit = id => {\n      const content = prompt(\"내용을 수정해주세요.\", state.data[idx]);\n      if (content === null) return;\n      if (content === \"\") return;\n      axios.put(\"/api/memos/\" + id, {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n    axios.get(\"/api/memos\").then(res => {\n      console.log(res);\n      state.data = res.data;\n    });\n    return {\n      state,\n      add,\n      edit\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","setup","state","data","add","content","prompt","post","then","res","console","log","edit","id","idx","put","get"],"sources":["C:\\Users\\kahn\\Desktop\\VUE\\VueJs\\firstapp\\src\\components\\myMemo.vue"],"sourcesContent":["<template>\r\n    <div class=\"memo\">\r\n        <div class=\"act\">\r\n            <button class=\"btn btn-primary btn-block\" @click=\"add()\">+ 추가</button>\r\n        </div>\r\n        \r\n        <ul>\r\n            <li v-for=\"d in state.data\" :key=\"d.id\" @click=\"edit(d.id)\" >{{ d.content}}</li>\r\n \r\n        </ul>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from 'vue'\r\nimport axios from \"axios\"\r\n\r\nexport default {\r\n    setup() {\r\n        const state = reactive({\r\n            data : [ ]\r\n        })\r\n        \r\n        const add =() =>{\r\n            const content = prompt(\"내용을 입력해주세요.\");\r\n            axios.post(\"/api/memos\", {content}).then((res) =>{\r\n                state.data = res.data;\r\n                console.log(res.data);\r\n            })\r\n        }\r\n\r\n        const edit = (id) => {\r\n            const content = prompt(\"내용을 수정해주세요.\", state.data[idx]);\r\n            if(content === null) return;\r\n            if(content === \"\") return;\r\n            axios.put(\"/api/memos/\"+id, {content}).then((res) => {\r\n                state.data  = res.data;\r\n            })\r\n\r\n        }\r\n\r\n        axios.get(\"/api/memos\").then((res) => {\r\n            console.log(res);\r\n            state.data = res.data;\r\n        })\r\n\r\n        return {state, add, edit};\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.memo {\r\n    .act{\r\n        padding: 15px 5px 5px 15px;\r\n        text-align: right;\r\n    }\r\n    ul{\r\n        border-top: 1px solid #eee;\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 0;\r\n\r\n    li {\r\n        padding: 5px 15px;\r\n        margin: 5px;\r\n        border: 1px solid #eee;\r\n    }\r\n}}\r\n\r\n\r\n</style>"],"mappings":"AAeA,SAASA,QAAO,QAAS,KAAI;AAC7B,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACnBI,IAAG,EAAI;IACX,CAAC;IAED,MAAMC,GAAE,GAAGA,CAAA,KAAK;MACZ,MAAMC,OAAM,GAAIC,MAAM,CAAC,aAAa,CAAC;MACrCN,KAAK,CAACO,IAAI,CAAC,YAAY,EAAE;QAACF;MAAO,CAAC,CAAC,CAACG,IAAI,CAAEC,GAAG,IAAI;QAC7CP,KAAK,CAACC,IAAG,GAAIM,GAAG,CAACN,IAAI;QACrBO,OAAO,CAACC,GAAG,CAACF,GAAG,CAACN,IAAI,CAAC;MACzB,CAAC;IACL;IAEA,MAAMS,IAAG,GAAKC,EAAE,IAAK;MACjB,MAAMR,OAAM,GAAIC,MAAM,CAAC,aAAa,EAAEJ,KAAK,CAACC,IAAI,CAACW,GAAG,CAAC,CAAC;MACtD,IAAGT,OAAM,KAAM,IAAI,EAAE;MACrB,IAAGA,OAAM,KAAM,EAAE,EAAE;MACnBL,KAAK,CAACe,GAAG,CAAC,aAAa,GAACF,EAAE,EAAE;QAACR;MAAO,CAAC,CAAC,CAACG,IAAI,CAAEC,GAAG,IAAK;QACjDP,KAAK,CAACC,IAAG,GAAKM,GAAG,CAACN,IAAI;MAC1B,CAAC;IAEL;IAEAH,KAAK,CAACgB,GAAG,CAAC,YAAY,CAAC,CAACR,IAAI,CAAEC,GAAG,IAAK;MAClCC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAChBP,KAAK,CAACC,IAAG,GAAIM,GAAG,CAACN,IAAI;IACzB,CAAC;IAED,OAAO;MAACD,KAAK;MAAEE,GAAG;MAAEQ;IAAI,CAAC;EAC7B;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}